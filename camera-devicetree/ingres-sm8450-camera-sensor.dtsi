		regulator-max-microvolt = <2900000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 22 0>;
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&cam_sensor_f_avdd_active &cam_sensor_f_avdd_suspend>;
